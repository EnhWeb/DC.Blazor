@inherits DC.Bue.Base.BaseButtons
@if ( !ComponentMapper.HasRegistration( this ) )
{
    <CascadingValue Value="this">
        <div class="@ClassMapper.Class" role="@(Role== ButtonsRole.Toolbar? "toolbar" : "group")">
            @ChildContent
        </div>
    </CascadingValue>
}
else
{
    @RenderCustomComponent()
}