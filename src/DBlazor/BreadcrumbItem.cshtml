@inherits DBlazor.Base.BaseBreadcrumbItem
@if ( !ComponentMapper.HasRegistration( this ) )
{
    <CascadingValue Value=this>
        <li class="@ClassMapper.Class" aria-current="@(IsActive ? "page" : null)">
            @ChildContent
        </li>
    </CascadingValue>
}
else
{
    @RenderCustomComponent()
}